import java.util.*;
class Student{
    String name;
    int age;

    public void display(){
       System.out.println("Name: "+name);
       System.out.println("Age: "+age);
    }
    public void getInfor(){
        Scanner sc=new Scanner(System.in);
        name=sc.next();
        age=sc.nextInt();
    }
}
public class Main{
    public static void main(String[]args){
        Student s1=new Student();
        s1.getInfor();
        s1.display();
    }
}
